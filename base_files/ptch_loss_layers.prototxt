layer {
  name: "ptch_fc"
  type: "InnerProduct"
  bottom: "common_fc"
  top: "ptch_fc"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 2
    weight_filler {
      type: "gaussian"
      std: 0.005
    }
    bias_filler {
      type: "constant"
      value: 1
    }
  }
}
layer {
  name: "ptch_loss"
  type: "SoftmaxWithLoss"
  bottom: "ptch_fc"
  bottom: "ptch_label"
  top: "ptch_loss"
	loss_weight: 1.0
}
layer {
  name: "accuracy"
  type: "Accuracy"
  bottom: "ptch_fc"
  bottom: "ptch_label"
  top: "accuracy"
}
