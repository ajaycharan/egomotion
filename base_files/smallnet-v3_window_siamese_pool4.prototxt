  name: "concat_pool4"
  type: "Concat"
  bottom: "pool4"
  bottom: "pool4_p"
  top: "concat_pool4"
}
layer {
  name: "common_fc"
  type: "InnerProduct"
  bottom: "concat_pool4"
  top: "common_fc"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 512
    weight_filler {
      type: "gaussian"
      std: 0.005
    }
    bias_filler {
      type: "constant"
      value: 1
    }
  }
}
layer {
  name: "relu_common"
  type: "ReLU"
  bottom: "common_fc"
  top: "common_fc"
}
layer {
	name: "drop4"
	type: "Dropout"
	bottom: "common_fc"
	top: "common_fc"
	dropout_param {
		dropout_ratio: 0.5
	}	
}

